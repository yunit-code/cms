{
    "classId": "idm.component.oalayout.itabheader",
    "comName": "多页签容器",
    "className": "ITabHeader",
    "comType": "common",
    "comLangue": "vue",
    "compositeAttr": [
        {
            "type": "input",
            "layoutType": "inline",
            "text": "唯一标识",
            "bindKey": "ctrlId",
            "disabled": true,
            "default": "@[packageid]",
            "desc": "",
            "helpUrl": ""
        },
        {
            "type": "group",
            "text": "页签属性",
            "desc": "提供设置组件能达到展示效果的基本属性设置信息",
            "children": [
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "页签大小",
                    "bindKey": "size",
                    "dictionary": [
                        {
                            "label": "小",
                            "value": "small"
                        },
                        {
                            "label": "中",
                            "value": "default"
                        },
                        {
                            "label": "大",
                            "value": "large"
                        }
                    ],
                    "default": "default"
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "页签位置",
                    "bindKey": "tabPosition",
                    "dictionary": [
                        {
                            "label": "top",
                            "value": "top"
                        },
                        {
                            "label": "right",
                            "value": "right"
                        },
                        {
                            "label": "bottom",
                            "value": "bottom"
                        },
                        {
                            "label": "left",
                            "value": "left"
                        }
                    ],
                    "default": "top"
                },
                {
                    "type": "inputNumber",
                    "layoutType": "inline",
                    "text": "页签间距",
                    "bindKey": "tabBarGutter",
                    "maxLength": 999,
                    "minLength": 0,
                    "default": 32
                },
                {
                    "type": "inputNumberUnit",
                    "layoutType": "inline",
                    "text": "左内边距",
                    "desc": "每个页签的左内边距，一般建议横向tab才设置",
                    "bindKey": "tabLeftPadding",
                    "dictionary": [
                        {
                            "label": "px",
                            "value": "px"
                        },
                        {
                            "label": "%",
                            "value": "%"
                        },
                        {
                            "label": "em",
                            "value": "em"
                        }
                    ]
                },
                {
                    "type": "inputNumberUnit",
                    "layoutType": "inline",
                    "text": "右内边距",
                    "desc": "每个页签的右内边距，一般建议横向tab才设置",
                    "bindKey": "tabRightPadding",
                    "dictionary": [
                        {
                            "label": "px",
                            "value": "px"
                        },
                        {
                            "label": "%",
                            "value": "%"
                        },
                        {
                            "label": "em",
                            "value": "em"
                        }
                    ]
                },
                {
                    "type": "inputNumberUnit",
                    "layoutType": "inline",
                    "text": "上内边距",
                    "desc": "每个页签的上内边距，一般建议竖向tab才设置",
                    "bindKey": "tabTopPadding",
                    "dictionary": [
                        {
                            "label": "px",
                            "value": "px"
                        },
                        {
                            "label": "%",
                            "value": "%"
                        },
                        {
                            "label": "em",
                            "value": "em"
                        }
                    ]
                },
                {
                    "type": "inputNumberUnit",
                    "layoutType": "inline",
                    "text": "下内边距",
                    "desc": "每个页签的下内边距，一般建议竖向tab才设置",
                    "bindKey": "tabBottomPadding",
                    "dictionary": [
                        {
                            "label": "px",
                            "value": "px"
                        },
                        {
                            "label": "%",
                            "value": "%"
                        },
                        {
                            "label": "em",
                            "value": "em"
                        }
                    ]
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "风格样式",
                    "bindKey": "type",
                    "dictionary": [
                        {
                            "label": "line",
                            "value": "line"
                        },
                        {
                            "label": "card",
                            "value": "card"
                        }
                    ],
                    "default": "line"
                },
                {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "切换动画",
                    "desc": "是否使用动画切换 Tabs，在 页签位置=顶部|底部 时有效",
                    "bindKey": "animated",
                    "default": true
                },
                {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "选中加粗",
                    "bindKey": "tabActiveBold",
                    "default": false
                },
                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "文字",
                    "bindKey": "tabFont"
                },
                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "悬浮文字",
                    "bindKey": "tabFontHover"
                },
                {
                    "type": "group",
                    "text": "角标样式",
                    "desc": "提供设置组件的页签上面的角标属性设置信息",
                    "children": [
                        {
                            "type": "inlineGroup",
                            "text": "角标偏移",
                            "children": [
                                {
                                    "type": "inputNumber",
                                    "layoutType": "block",
                                    "text": "偏移(X)",
                                    "desc": "单位为px，默认为跟在文字的后面，可以通过调整此数值来改变横向的偏移位置",
                                    "bindKey": "remindOffsetX",
                                    "width": "60px",
                                    "default": 0
                                },
                                {
                                    "type": "inputNumber",
                                    "layoutType": "block",
                                    "text": "偏移(Y)",
                                    "desc": "单位为px，默认为顶部对齐，可以通过调整此数值来改变纵向的偏移位置",
                                    "bindKey": "remindOffsetY",
                                    "width": "60px",
                                    "default": 0
                                }
                            ]
                        },
                        {
                            "type": "colorPicker",
                            "layoutType": "inline",
                            "text": "角标颜色",
                            "bindKey": "remindBgColor",
                            "placeholder": "请选择颜色",
                            "default": {
                                "hex": "#ec4519",
                                "hex8": "#ec4519FF"
                            }
                        },
                        {
                            "type": "switch",
                            "layoutType": "inline",
                            "text": "显示红点",
                            "bindKey": "remindShowDot",
                            "desc": "设置未选中的tab是否显示红点不显示具体的数值",
                            "default": true
                        },
                        {
                            "type": "inputNumber",
                            "layoutType": "inline",
                            "text": "数字Max",
                            "desc": "角标数字显示的最大值，如果超过这个值则会显示+号",
                            "bindKey": "remindNumberMax",
                            "default": 99
                        }
                    ]
                },
                {
                    "type": "group",
                    "text": "分割线样式",
                    "desc": "提供设置组件的页签之间的分割线属性设置信息",
                    "children": [
                        {
                            "type": "switch",
                            "layoutType": "inline",
                            "text": "显示分割",
                            "bindKey": "tabShowDivider",
                            "default": true
                        },
                        {
                            "type": "inputNumber",
                            "layoutType": "inline",
                            "text": "高度占比",
                            "desc": "单位为%，默认为100撑满，只能填写基于tab高度的百分比",
                            "bindKey": "dividerHeightNumber",
                            "display": "@[tabShowDivider]",
                            "default": 100
                        },
                        {
                            "type": "inputNumber",
                            "layoutType": "inline",
                            "text": "偏移值(Y)",
                            "desc": "单位为%，默认为顶部对齐，只能填写基于tab高度的百分比",
                            "bindKey": "dividerTopNumber",
                            "display": "@[tabShowDivider]",
                            "default": 0
                        },
                        {
                            "type": "inputNumber",
                            "layoutType": "inline",
                            "text": "偏移值(X)",
                            "desc": "单位为px，默认线为文字的右边，如果要tab之间可以参考页签间距的二分之一的大小",
                            "bindKey": "dividerRightNumber",
                            "display": "@[tabShowDivider]",
                            "default": -16
                        },
                        {
                            "type": "colorPicker",
                            "layoutType": "inline",
                            "text": "分线颜色",
                            "bindKey": "dividerBgColor",
                            "display": "@[tabShowDivider]",
                            "placeholder": "请选择颜色",
                            "default": {
                                "hex": "#e8e8e8",
                                "hex8": "#e8e8e8FF"
                            }
                        }
                    ]
                },
                {
                    "type": "group",
                    "text": "选中样式",
                    "desc": "提供设置组件选中页签的样式",
                    "children": [
                        {
                            "type": "switch",
                            "layoutType": "inline",
                            "text": "选中下划线",
                            "bindKey": "chooseShowBorder",
                            "default": true
                        },
                        {
                            "type": "border",
                            "layoutType": "block",
                            "text": "边框",
                            "bindKey": "chooseBorder"
                        },
                        {
                            "type": "colorPicker",
                            "layoutType": "inline",
                            "text": "背景色",
                            "bindKey": "bgColorChoose",
                            "placeholder": "请选择颜色",
                            "default": {}
                        },
                        {
                            "type": "font",
                            "layoutType": "block",
                            "text": "选中文字",
                            "bindKey": "tabFontChoose"
                        }
                    ]
                }
            ]
        },
        {
            "type": "table",
            "text": "页签选项设置",
            "bindKey": "staticTabPaneList",
            "tabText": "页签[index]",
            "children": [
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "tab标识",
                    "bindKey": "key"
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "tab文字",
                    "bindKey": "tab"
                },
                {
                    "type": "actionSelect",
                    "layoutType": "block",
                    "text": "tab显示自定义函数",
                    "bindKey": "tabSlotFunction",
                    "desc": "tab页签内容自定义函数，支持返回HTML格式字符串，接收参数为格式为{...自定义的,当前页签数据对象}，返回html字符串",
                    "multiple": false
                },
                {
                    "type": "inputNumber",
                    "layoutType": "inline",
                    "text": "红点数字",
                    "bindKey": "cnt",
                    "desc": "红点显示个数",
                    "default": 10
                },
                {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "默认选中",
                    "bindKey": "defaultActiveKey",
                    "default": false
                },
                {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "懒加载",
                    "bindKey": "forceRender",
                    "desc": "选用此项则被隐藏的tab默认不渲染dom，点击的时候第一次去渲染tab",
                    "default": false
                },
                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "文字",
                    "bindKey": "tableFont"
                }
            ]
        },
        {
            "type": "group",
            "text": "页签右边扩展按钮属性",
            "desc": "提供设置组件的页签右边扩展按钮设置信息",
            "children": [
                {
                    "type": "inputNumber",
                    "layoutType": "inline",
                    "text": "按钮间距",
                    "bindKey": "extraBtnGutter",
                    "desc": "按钮间距的数值是会平均分配给每个按钮的左右两边的内边距的，建议设置数值为偶数",
                    "maxLength": 50,
                    "minLength": 0,
                    "default": 20
                },
                {
                    "type": "inputNumber",
                    "layoutType": "inline",
                    "text": "文字大小",
                    "bindKey": "extraBtnFontSize",
                    "desc": "按钮文字大小，包括图标的大小",
                    "maxLength": 30,
                    "minLength": 11,
                    "default": 14
                },
                {
                    "type": "colorPicker",
                    "layoutType": "inline",
                    "text": "默认颜色",
                    "bindKey": "extraBtnFontColor",
                    "desc": "按钮文字和图标的默认颜色，默认会以透明度0.7的比例显示，请合理填写",
                    "placeholder": "请选择颜色",
                    "default": {
                        "hex": "#999999",
                        "hex8": "#999999FF"
                    }
                },
                {
                    "type": "table",
                    "text": "扩展按钮设置",
                    "bindKey": "extraBtnList",
                    "tabText": "操作[index]",
                    "children": [
                        {
                            "type": "input",
                            "layoutType": "inline",
                            "text": "按钮标识",
                            "desc": "主要用于标识按钮，可以在需要使用的时候使用此标识进行判断，建议设置为唯一或者按钮的英文、拼音作为标识",
                            "bindKey": "key"
                        },
                        {
                            "type": "input",
                            "layoutType": "inline",
                            "text": "显示名称",
                            "desc": "按钮要显示的名称",
                            "bindKey": "name"
                        },
                        {
                            "type": "iconSelect",
                            "layoutType": "inline",
                            "text": "显示图标",
                            "bindKey": "icon",
                            "desc": "按钮显示名称的图标显示",
                            "placeholder": "请选择"
                        },
                        {
                            "type": "input",
                            "layoutType": "inline",
                            "text": "悬浮提示",
                            "desc": "鼠标放到按钮上显示的提示",
                            "bindKey": "tip"
                        },
                        {
                            "type": "select",
                            "layoutType": "inline",
                            "text": "显示方式",
                            "bindKey": "showType",
                            "desc": "可以根据切换显示方式对每个按钮进行更高级的控制，更多使用方法请点击参考更详细的说明",
                            "dictionary": [
                                {
                                    "label": "始终显示",
                                    "value": "default"
                                },
                                {
                                    "label": "切换依据显示",
                                    "value": "toggle"
                                },
                                {
                                    "label": "自定义函数显示",
                                    "value": "custom"
                                }
                            ],
                            "default": "default"
                        },
                        {
                            "type": "input",
                            "layoutType": "inline",
                            "text": "显示依据",
                            "desc": "当显示方式为“切换依据显示”的时候代表页签选项卡切换的时候会根据当前填写的依据字段来去匹配结果值，为true则显示，false则不显示，此处填写表达式，更多详细用法点击查看",
                            "bindKey": "dataFiled",
                            "display": "@[showType=='toggle']",
                            "placeholder": "示例：moreUrl 或者 [0].showSetting"
                        },
                        {
                            "type": "actionSelect",
                            "layoutType": "block",
                            "text": "自定义函数依据显示",
                            "bindKey": "dataFunction",
                            "display": "@[showType=='custom']",
                            "desc": "切换选项卡、数据勾选等动作会触发回调来控制是否显示，动态属性模式下也会调用此函数，接收参数为格式为{...url参数,customParam:自定义的,activeKey,allKey,actionType:动作类型,otherData}",
                            "multiple": false
                        },
                        {
                            "type": "select",
                            "layoutType": "inline",
                            "text": "点击事件",
                            "bindKey": "clickType",
                            "desc": "设置按钮点击响应事件的类型，更多使用方法请点击参考更详细的说明",
                            "dictionary": [
                                {
                                    "label": "不响应",
                                    "value": "none"
                                },
                                {
                                    "label": "自定义动作函数",
                                    "value": "customFunction"
                                },
                                {
                                    "label": "打开控制中心",
                                    "value": "openCtrlCenter"
                                }
                            ],
                            "default": "customFunction"
                        },
                        {
                            "type": "actionSelect",
                            "layoutType": "block",
                            "text": "点击自定义函数",
                            "bindKey": "clickCustomFunction",
                            "display": "@[clickType=='customFunction']",
                            "desc": "当点击按钮时候会调用此函数，接收参数为格式为{...url参数,customParam:自定义的,activeKey,allKey,actionType:动作类型,allTabDataList}",
                            "multiple": true
                        }
                    ]
                }
            ]
        },
        {
            "type": "group",
            "text": "基础样式",
            "desc": "点击？查看样式设置用法指南",
            "helpUrl": "",
            "children": [
                {
                    "type": "box",
                    "layoutType": "block",
                    "desc": "组件的最外围的内外边距设置",
                    "text": "组件内外边距",
                    "bindKey": "box"
                },
                {
                    "type": "inlineGroup",
                    "text": "宽高",
                    "children": [
                        {
                            "type": "input",
                            "layoutType": "block",
                            "text": "宽",
                            "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                            "bindKey": "width",
                            "width": "60px",
                            "default": "auto"
                        },
                        {
                            "type": "input",
                            "layoutType": "block",
                            "text": "高",
                            "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                            "bindKey": "height",
                            "width": "60px",
                            "default": "auto"
                        }
                    ]
                },
                {
                    "type": "group",
                    "text": "背景设置",
                    "desc": "点击？查看背景设置用法指南",
                    "helpUrl": "",
                    "children": [
                        {
                            "type": "colorPicker",
                            "layoutType": "inline",
                            "text": "背景色",
                            "bindKey": "bgColor",
                            "placeholder": "请选择颜色",
                            "default": {}
                        },
                        {
                            "type": "uploadImage",
                            "layoutType": "inline",
                            "text": "背景图片",
                            "bindKey": "bgImgUrl",
                            "placeholder": "可输入图片地址或直接上传"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "横向偏移",
                            "bindKey": "positionX",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgImgUrl]"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "纵向偏移",
                            "bindKey": "positionY",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgImgUrl]"
                        },
                        {
                            "type": "radio",
                            "layoutType": "block",
                            "text": "背景大小",
                            "bindKey": "bgSize",
                            "dictionary": [
                                {
                                    "label": "裁切显示",
                                    "value": "cover"
                                },
                                {
                                    "label": "完全显示",
                                    "value": "contain"
                                },
                                {
                                    "label": "自定义",
                                    "value": "custom"
                                }
                            ],
                            "display": "@[bgImgUrl]"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "宽度",
                            "bindKey": "bgSizeWidth",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgSize=='custom']"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "高度",
                            "bindKey": "bgSizeHeight",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgSize=='custom']"
                        },
                        {
                            "type": "radio",
                            "layoutType": "block",
                            "text": "平铺模式",
                            "bindKey": "bgRepeat",
                            "dictionary": [
                                {
                                    "label": "双向重复",
                                    "value": "repeat"
                                },
                                {
                                    "label": "水平重复",
                                    "value": "repeat-x"
                                },
                                {
                                    "label": "垂直重复",
                                    "value": "repeat-y"
                                },
                                {
                                    "label": "不重复",
                                    "value": "no-repeat"
                                },
                                {
                                    "label": "继承",
                                    "value": "inherit"
                                }
                            ],
                            "display": "@[bgImgUrl]"
                        },
                        {
                            "type": "radio",
                            "layoutType": "inline",
                            "text": "背景模式",
                            "bindKey": "bgAttachment",
                            "dictionary": [
                                {
                                    "label": "固定",
                                    "value": "fixed"
                                },
                                {
                                    "label": "滚动",
                                    "value": "scroll"
                                },
                                {
                                    "label": "继承",
                                    "value": "inherit"
                                }
                            ],
                            "display": "@[bgImgUrl]"
                        }
                    ]
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "阴影样式",
                    "desc": "填写格式如下：10px 10px 5px #888888，更多请点击查看详细填写说明",
                    "bindKey": "boxShadow",
                    "default": "0px 0px 3px rgba(0,0,0,0.1)"
