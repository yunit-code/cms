{
    "classId": "idm.component.cms.datepicker",
    "comName": "日期选择器",
    "className": "IDatePicker",
    "comType": "common",
    "comLangue": "vue",
    "compositeAttr": [
        {
            "type": "input",
            "layoutType": "inline",
            "text": "唯一标识",
            "bindKey": "ctrlId",
            "disabled": true,
            "default": "@[packageid]",
            "desc": "",
            "helpUrl": ""
        },
        {
            "type": "group",
            "text": "基本属性",
            "children": [
                {
                    "type": "radio",
                    "styleType": "radio",
                    "layoutType": "inline",
                    "text": "显示标题",
                    "bindKey": "showLabel",
                    "default": true,
                    "dictionary": [
                        {
                            "label": "是",
                            "value": true
                        },
                        {
                            "label": "否",
                            "value": false
                        }
                    ]
                },
                {
                    "type": "radio",
                    "styleType": "radio",
                    "layoutType": "inline",
                    "text": "显示清空",
                    "bindKey": "clearable",
                    "default": true,
                    "dictionary": [
                        {
                            "label": "是",
                            "value": true
                        },
                        {
                            "label": "否",
                            "value": false
                        }
                    ]
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "标题",
                    "bindKey": "label",
                    "default": "选择时间",
                    "display":"@[showLabel]"
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "默认值",
                    "bindKey": "defaultValue",
                    "default": ""
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "值格式",
                    "bindKey": "valueFormat",
                    "default": "yyyy-MM-dd",
                    "desc": "可选，绑定值的格式"
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "显示格式",
                    "bindKey": "format",
                    "default": "yyyy-MM-dd",
                    "desc": "可选，绑定值的格式"
                },
                {
                    "type": "select",
                    "layoutType": "inline",
                    "text": "显示类型",
                    "bindKey": "showType",
                    "dictionary": [
                        {
                            "label": "date",
                            "value": "date"
                        },
                        {
                            "label": "month",
                            "value": "month"
                        },
                        {
                            "label": "year",
                            "value": "year"
                        },
                        {
                            "label": "dates",
                            "value": "dates"
                        },
                        {
                            "label": "months",
                            "value": "months"
                        },
                        {
                            "label": "years",
                            "value": "years"
                        }
                    ],
                    "default": "date"
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "尺寸",
                    "bindKey": "size",
                    "dictionary": [
                        {
                            "label": "小",
                            "value": "small"
                        },
                        {
                            "label": "中",
                            "value": "default"
                        },
                        {
                            "label": "大",
                            "value": "large"
                        }
                    ],
                    "default": "default"
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "排列方式",
                    "bindKey": "radioDirection",
                    "dictionary": [
                        {
                            "label": "横向",
                            "value": "horizontal"
                        },
                        {
                            "label": "纵向",
                            "value": "vertical"
                        }
                    ],
                    "default": "horizontal"
                }
            ]
        },
        {
            "type": "group",
            "text": "元素样式设置",
            "children":[
                {
                    "type": "group",
                    "text": "标题样式设置",
                    "children":[
                        {
                            "type": "input",
                            "layoutType": "inline",
                            "text": "宽",
                            "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                            "bindKey": "widthLabel",
                            "default": "100%"
                        },
                        {
                            "type": "input",
                            "layoutType": "inline",
                            "text": "高",
                            "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                            "bindKey": "heightLabel",
                            "default": "auto"
                        },
                        {
                            "type": "font",
                            "layoutType": "block",
                            "text": "文字",
                            "bindKey": "fontLabel"
                        },
                        {
                            "type": "box",
                            "layoutType": "block",
                            "text": "内外边距",
                            "bindKey": "boxLabel"
                        }
                    ]
                },
                {
                    "type": "group",
                    "text": "选择时间框样式",
                    "children":[
                        {
                            "type": "input",
                            "layoutType": "inline",
                            "text": "宽",
                            "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                            "bindKey": "widthDateBox",
                            "default": ""
                        },
                        {
                            "type": "colorPicker",
                            "layoutType": "inline",
                            "text": "边框颜色",
                            "bindKey": "borderColorInput",
                            "placeholder": "请选择颜色",
                            "default": {}
                        },
                        {
                            "type": "colorPicker",
                            "layoutType": "inline",
                            "text": "图标颜色",
                            "bindKey": "colorIcon",
                            "placeholder": "请选择颜色",
                            "default": {}
                        },
                        {
                            "type": "colorPicker",
                            "layoutType": "block",
                            "text": "悬浮边框颜色",
                            "bindKey": "borderColorInputHover",
                            "placeholder": "请选择颜色",
                            "default": {}
                        },
                        {
                            "type": "font",
                            "layoutType": "block",
                            "text": "文字",
                            "bindKey": "fontInput"
                        }
                    ]
                }
            ]
        },
        {
            "type": "group",
            "text": "容器样式设置",
            "desc": "点击？查看样式设置用法指南",
            "helpUrl": "",
            "children": [
                {
                    "type": "box",
                    "layoutType": "block",
                    "text": "内外边距",
                    "bindKey": "box"
                },
                {
                    "type": "inlineGroup",
                    "text": "宽高",
                    "children": [
                        {
                            "type": "input",
                            "layoutType": "block",
                            "text": "宽",
                            "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                            "bindKey": "width",
                            "width": "60px",
                            "default": "100%"
                        },
                        {
                            "type": "input",
                            "layoutType": "block",
                            "text": "高",
                            "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                            "bindKey": "height",
                            "width": "60px",
                            "default": "auto"
                        }
                    ]
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "最小宽度",
                    "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                    "bindKey": "minWidth",
                    "default": ""
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "最小高度",
                    "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                    "bindKey": "minHeight",
                    "default": ""
                },
                {
                    "type": "group",
                    "text": "背景设置",
                    "desc": "点击？查看背景设置用法指南",
                    "helpUrl": "",
                    "children": [
                        {
                            "type": "colorPicker",
                            "layoutType": "inline",
                            "text": "背景色",
                            "bindKey": "bgColor",
                            "placeholder": "请选择颜色",
                            "default": {}
                        },
                        {
                            "type": "uploadImage",
                            "layoutType": "inline",
                            "text": "背景图片",
                            "bindKey": "bgImgUrl",
                            "placeholder": "可输入图片地址或直接上传"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "横向偏移",
                            "bindKey": "positionX",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgImgUrl]"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "纵向偏移",
                            "bindKey": "positionY",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgImgUrl]"
                        },
                        {
                            "type": "radio",
                            "layoutType": "block",
                            "text": "背景大小",
                            "bindKey": "bgSize",
                            "dictionary": [
                                {
                                    "label": "裁切显示",
                                    "value": "cover"
                                },
                                {
                                    "label": "完全显示",
                                    "value": "contain"
                                },
                                {
                                    "label": "自定义",
                                    "value": "custom"
                                }
                            ],
                            "display": "@[bgImgUrl]"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "宽度",
                            "bindKey": "bgSizeWidth",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgSize=='custom']"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "高度",
                            "bindKey": "bgSizeHeight",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgSize=='custom']"
                        },
                        {
                            "type": "radio",
                            "layoutType": "block",
                            "text": "平铺模式",
                            "bindKey": "bgRepeat",
                            "dictionary": [
                                {
                                    "label": "双向重复",
                                    "value": "repeat"
                                },
                                {
                                    "label": "水平重复",
                                    "value": "repeat-x"
                                },
                                {
                                    "label": "垂直重复",
                                    "value": "repeat-y"
                                },
                                {
                                    "label": "不重复",
                                    "value": "no-repeat"
                                },
                                {
                                    "label": "继承",
                                    "value": "inherit"
                                }
                            ],
                            "display": "@[bgImgUrl]"
                        },
                        {
                            "type": "radio",
                            "layoutType": "inline",
                            "text": "背景模式",
                            "bindKey": "bgAttachment",
                            "dictionary": [
                                {
                                    "label": "固定",
                                    "value": "fixed"
                                },
                                {
                                    "label": "滚动",
                                    "value": "scroll"
                                },
                                {
                                    "label": "继承",
                                    "value": "inherit"
                                }
                            ],
                            "display": "@[bgImgUrl]"
                        }
                    ]
                },
                {
                    "type": "border",
                    "layoutType": "block",
                    "text": "边框",
                    "bindKey": "border"
                },
                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "文字",
                    "bindKey": "font"
                }
            ]
        },
        {
            "type": "table",
            "text": "主题设置",
            "desc": "点击？查看主题设置用法指南",
            "bindKey": "themeList",
            "tabText": "主题[index]",
            "default":[
                {
                    "key": "blue",
                    "mainColor": {
                        "hex": "#0073CA",
                        "hex8": "#0073CAFF"
                    },
                    "minorColor": {
                        "hex": "#329cec",
                        "hex8": "#329cecFF"
                    }
                },
                {
                    "key": "red",
                    "mainColor": {
                        "hex": "#E21A1A",
                        "hex8": "#E21A1AFF"
                    },
                    "minorColor": {
                        "hex": "#f87373",
                        "hex8": "#f87373FF"
                    }
                },
                {
                    "key": "green",
                    "mainColor": {
                        "hex": "#0EAF64",
                        "hex8": "#0EAF64FF"
                    },
                    "minorColor": {
                        "hex": "#38e996",
                        "hex8": "#38e996FF"
                    }
                }
            ],
            "children": [
                {
                    "type": "select",
                    "layoutType": "inline",
                    "text": "主题标识",
                    "bindKey": "key",
                    "desc":"用于设置主题的标识，此项需要搭配自定义开发切换主题的功能，例如：蓝色标识为blue，用户切换主题的时候存储到数据库中的标识为blue，则这里选择blue",
                    "default":"",
                    "placeholder":"例如：blue",
                    "ctrlAttrObject": {
                        "mode": "combobox"
                    },
                    "dictionary": [
                        {
                            "label": "red",
                            "value": "red"
                        },
                        {
                            "label": "green",
                            "value": "green"
                        },
                        {
                            "label": "blue",
                            "value": "blue"
                        }
                    ]
                },
                {
                    "type": "colorPicker",
                    "layoutType": "inline",
                    "text": "主要颜色",
                    "bindKey": "mainColor",
                    "desc":"主题的主要颜色",
                    "placeholder": "请选择颜色",
                    "display":"@[key]"
                },
                {
                    "type": "colorPicker",
                    "layoutType": "inline",
                    "text": "次要颜色",
                    "bindKey": "minorColor",
                    "desc": "主题的次要颜色，作用于菜单鼠标悬浮色、选中菜单颜色",
                    "placeholder": "请选择颜色",
                    "display": "@[key]"
                },
                {
                    "type": "uploadImage",
                    "layoutType": "inline",
                    "text": "背景图片",
                    "bindKey": "bgImgSrc",
                    "default": ""
                }
            ]
        },
        {
            "type": "group",
            "text": "高级",
            "desc": "提供设置组件能达到展示效果的基本属性设置信息",
            "children": [
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "表单标识",
                    "bindKey": "formFiledKey",
                    "desc": "如果为空则会提交表单时用唯一标识作为key",
                    "default": ""
                },
                {
                    "type": "group",
                    "text": "联动组件",
                    "desc": "当选择的数据有变动会联动这里选择的组件，把当前选中的对象传递给这些组件",
                    "children": [
                        {
                            "type": "pageModuleSelect",
                            "layoutType": "block",
                            "text": "需求组件范围",
                            "desc": "当前选项变更需要联动其他组件需求值（比如下拉选项数据等）的组件范围",
                            "bindKey": "linkageDemandPageModule",
                            "multiple": true
                        },
                        {
                            "type": "pageModuleSelect",
                            "layoutType": "block",
                            "text": "结果组件范围",
                            "desc": "当前选项变更需要联动其他组件变更结果值的组件范围",
                            "bindKey": "linkageResultPageModule",
                            "multiple": true
                        }
                    ]
                }
            ]
        }
    ]
}